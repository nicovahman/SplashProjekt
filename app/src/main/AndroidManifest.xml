<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nicolasvahman.splashprojekt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hangman - 2018"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:replace="android:label"

        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".TheGame" />
        <activity android:name=".Highscore" />
        <activity android:name=".AlmindeligtSpil" />
        <activity android:name=".vinderscreen" />
        <activity android:name=".taberscreen" />
        <activity android:name=".guessGame" />
        <activity
            android:name=".guess2"
            android:parentActivityName=".guessGame" />
        <activity android:name=".omSpillet"></activity>
    </application>

</manifest>